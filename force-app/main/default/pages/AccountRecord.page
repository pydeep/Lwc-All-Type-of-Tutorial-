<apex:page standardController="Account" recordSetVar="accounts">

<apex:pageBlock >
    <apex:pageBlockTable value="{!accounts}" var="object">
        <apex:column headerValue="Name" value="{!object.Name}"/>
        <apex:column headerValue="Industry" value="{!object.Industry}"/>
        <apex:column headerValue="Type" value="{!object.Type}"/>
    </apex:pageBlockTable>    

</apex:pageBlock>
</apex:page>